<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="232.0" prefWidth="309.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MyController.LoginController">
    <children>
        <Button fx:id="loginB" layoutX="210.0" layoutY="116.0" mnemonicParsing="false" onAction="#login" prefHeight="27.0" prefWidth="61.0" text="Login" />
        <Button fx:id="exitB" layoutX="180.0" layoutY="185.0" mnemonicParsing="false" onAction="#exit" prefHeight="28.0" prefWidth="61.0" text="Exit" />
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="34.0" layoutY="41.0" prefHeight="19.0" prefWidth="75.0" text="User name" />
        <Label layoutX="46.0" layoutY="79.0" text="Password" />
        <CheckBox layoutX="41.0" layoutY="120.0" mnemonicParsing="false" text="Remember password" />
        <PasswordField fx:id="passwordField" layoutX="111.0" layoutY="76.0" onAction="#login" promptText="password" text="admin" />
        <TextField fx:id="usernameField" layoutX="111.0" layoutY="37.0" onAction="#login" promptText="reader name" text="admin"/>
      <Button fx:id="registerB" layoutX="66.0" layoutY="186.0" mnemonicParsing="false" onAction="#register" text="Register" />
      <Label fx:id="status" alignment="CENTER" contentDisplay="CENTER" layoutX="9.0" layoutY="153.0" prefHeight="19.0" prefWidth="290.0" />
    </children>
</AnchorPane>
